        <Button id="AdjustBBut" height="80" width="80" class="ArcButtons" position="0 0 -5" active="true" onClick="setupShift">
            <Image id="AdjustB" image="AdjustB" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
        <Button id="AdjustLBut" height="80" width="80" class="ArcButtons" position="90 -35 -5" active="true" onClick="setupShift">
            <Image id="AdjustL" image="AdjustL" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
        <Button id="AdjustRBut" height="80" width="80" class="ArcButtons" position="-90 -35 -5" active="true" onClick="setupShift">
            <Image id="AdjustR" image="AdjustR" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
</Panel>

<Panel id="CenterFD"
    active="false"
    width="300"
    height="300"
    position="0 0 -25"
    rotation="180 180 180"
    color="#aaffaa00">
        <Button id="moveWindow" class="CenterButtons" height="75" width="100" position="0 10 -2" active="true" onClick="Window">Moves</Button>
        <Button id="setBtnFD" class="CenterButtons" height="75" position="0 10 -2" active="false" onClick="setDial" onMouseEnter="showSetMan" onMouseExit="hideSetMan">Set</Button>
        <Text   id="Name" class="DialName" position="0 70 3" rotation="180 180 0" height="80" fontSize="25" fontStyle="Bold">Name</Text>
        <Button id="TokenMenu" class="CenterMenuButtons" offsetXY="-110 15" active="true" onClick="menuClick" onMouseEnter="menuHover">T</Button>
        <Button id="ArcMenu" class="CenterMenuButtons" offsetXY="110 15" active="true" onClick="menuClick" onMouseEnter="menuHover">A</Button>
        <Button id="undoFD" class="CenterMenuButtons" offsetXY="0 -51" active="true" onClick="Undo" height="30" width="60" fontSize="18" fontStyle="Bold">Undo</Button>
        <Button id="RelocMenu" class="CenterMenuButtons" offsetXY="0 -105" active="true" onClick="menuClick" onMouseEnter="menuHover">R</Button>
</Panel>

