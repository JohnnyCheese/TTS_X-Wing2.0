<!-- Defaults apply styles to every element with the same class -->
<Defaults>
    <Button class="TokenButtons" height="50" width="50" color="#aaffaa00" active="false"/>
    <Button class="RelocButtons" height="60" width="60" preferredWidth="60" preferredHeight="60" minWidth="60" minHeight="60" color="#aaffaa00" active="false"/>
    <Button class="ArcButtons" height="80" width="80" color="#aaffaa00"/>
    <Button class="DarkButton" color="#222222" outline="#BBBBBB" outlineSize="2 2" fontSize="22" textColor="#FFFFFF" fontStyle="Bold"/>
    <Button class="CenterMenuButtons" rotation="0 0 180" height="60" width="60" color="#222222" outline="#BBBBBB" outlineSize="2 2" fontSize="22" textColor="#FFFFFF" fontStyle="Bold"/>
    <Button class="topButton" width="40" height="40" rectAlignment="UpperRight"/>
    <Button class="CenterButtons" fontSize="22" fontStyle="Bold" rotation="0 0 180" color="#222222" outline="#BBBBBB" outlineSize="2 2" textColor="#FFFFFF" height="40" width="80" active="false"/>

    <ToggleButton class="ManBtn" height="75" width="75" active="false" preferredWidth="60" preferredHeight="60" onValueChanged="manSelect"  outline="#BBBBBB" colors="#888888|Orange|Red|Orange" deselectedBackgroundColor="#555555" selectedBackgroundColor="#B24915"/>

    <Panel class="Window" color="#aaffaa00" width="1000" height="800" rectAlignment="MiddleRight"/>
    <Panel class="TopBar" color="#aaffaa00" height="30" rectAlignment="UpperCenter"/>
    <Panel class="BoostPanel" color="#aaffaa00" preferredWidth="200" preferredHeight="100" flexibleWidth="0"/>
    <Panel class="TalonP" color="#aaffaa00" rotation="0 0 0" height="100" width="350"/>
    <Panel class="BarrelPanel" color="Green" minWidth="80" flexibleWidth="false"/>
    <Panel class="WinElements" color="#aaffaa00" width="600" height="550" rectAlignment="UpperCenter"/>

    <Image class="TIcon" color="#ffffff"/>
    <Image class="RIcon" height="60" width="60" color="#ffffff"/>
    <Image class="MIcon" />
    <Image class="BGImage" color="#ffffff"/>
    <Image class="SpeedImg" height="85" width="85" />
    <Image class="SetIcon" rotation="0 0 180" height="55" width="55" preserveAspect="true"/>
</Defaults>

<Panel id="Setup"
    active="false"
    width="300"
    height="300"
    position="0 0 -25"
    rotation="180 180 180"
    color="#aaffaa00">
        <Button id="placeShip" class="CenterButtons" height="100" width="160" position="0 170 -5" active="true" onClick="placeShip">Place Ship</Button>
        <Text id="SetupName" class="DialName" position="0 70 3" rotation="180 180 0" height="80" fontSize="25" fontStyle="Bold">Name</Text>
        <Button id="allignShip" class="CenterMenuButtons" position="0 -180 -5" active="true" onClick="allignShip" height="90" width="90" fontSize="18" fontStyle="Bold">Align</Button>
        <Button id="Rot1DegLBut" height="80" width="80" class="ArcButtons" position="90 -175 -5" active="true" onClick="setupRot">
            <Image id="Rot1DegL" image="Rot1DegL" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
        <Button id="Rot1DegRBut" height="80" width="80" class="ArcButtons" position="-90 -175 -5" active="true" onClick="setupRot">
            <Image id="Rot1DegR" image="Rot1DegR" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
        <Button id="Rot5DegLBut" height="80" width="80" class="ArcButtons" position="165 -100 -5" active="true" onClick="setupRot">
            <Image id="Rot5DegL" image="Rot5DegL" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
        <Button id="Rot5DegRBut" height="80" width="80" class="ArcButtons" position="-165 -100 -5" active="true" onClick="setupRot">
            <Image id="Rot5DegR" image="Rot5DegR" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
        <Button id="Rot225DegLBut" height="80" width="80" class="ArcButtons" position="185 0 -5" active="true" onClick="setupRot">
            <Image id="Rot225DegL" image="Rot225DegL" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
        </Button>
        <Button id="Rot225DegRBut" height="80" width="80" class="ArcButtons" position="-185 0 -5" active="true" onClick="setupRot">
            <Image id="Rot225DegR" image="Rot225DegR" height="80" width="80" class="SetIcon" preserveAspect="true"></Image>
