        offsetXY="230 0"
        color="#aaffaa00">
        <Button id="BarrelRBtn" class="RelocButtons" onClick="BarrelR">
            <Image id= "BarrelRIcon" class="RIcon" image="BarrelR" preserveAspect="true"></Image>
