    <Image id="TypeIcon" class="SetIcon" active="false" image="BlueStraight" offsetXY="-30 15" height="60" width="60"></Image>
</Panel>

<Panel id="arcPanel"
    active="false"
    width="400"
    height="500"
    color="#ffaaaa00"
    raycastTarget="true"
    onMouseExit="menuHover">
    <Panel id="ArcFD"
        width="380"
        height="400"
        outline="Black"
        color="#aaffaa00"
        showAnimation="FadeIn"
        hideAnimation="FadeOut">
            <Button id="Range1" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="-140 150">
                <Image id="R1Icon" height="80" width="80" image="R1"></Image></Button>
            <Button id="Range2" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="-40 150">
                <Image id="R2Icon" height="80" width="80" image="R2"></Image></Button>
            <Button id="Range3" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="60 150">
                <Image id="R3Icon" height="80" width="80" image="R3"></Image></Button>
            <Button id="FrontArc" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="50 50">
                <Image id="FrontArcIcon" height="80" width="80" image="FrontArc"></Image></Button>
            <Button id="LeftArc" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="-40 -40">
                <Image id="LeftArcIcon" height="80" width="80" image="LeftArc"></Image></Button>
            <Button id="RightArc" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="140 -40">
                <Image id="RightArcIcon" height="80" width="80" image="RightArc"></Image></Button>
            <Button id="RearArc" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="50 -130">
                <Image id="BackArcIcon" height="80" width="80" image="BackArc"></Image></Button>
            <Button id="BullseyeArc" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="50 -40">
                <Image id="BullseyeIcon" height="80" width="80" image="Bullseye"></Image></Button>
            <Button id="FullFrontArc" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="-140 50">
                <Image id="FullFrontArcIcon" height="80" width="80" image="FullFront"></Image></Button>
            <Button id="FullBackArc" class="ArcButtons" onClick="Arc" height="80" width="80" offsetXY="-140 -130">
                <Image id="FullBackArcIcon" height="80" width="80" image="FullBack"></Image></Button>
    </Panel>
</Panel>

<Panel id="setManPeekPanel"
    active="false"
    width="70"
    height="140"
    color="#555555ff"
    showAnimation="FadeIn"
    hideAnimation="FadeOut">
        <Image id="setManTypeIcon" height="60" width="60" image="BlueStraight" offsetXY="0 35"></Image>
        <Image id="setManSpeedIcon" height="60" width="60" image="Speed1" offsetXY="0 -35"></Image>
</Panel>

<Panel id="tokenPanel"
    active="false"
    width="600"
    height="450"
    color="#aaffaa00"
    raycastTarget="true"
    onMouseExit="menuHover">
    <Panel id="TokenButtonPanel"
        width="400"
        height="350"
        rectAlignment="TopRight"
        outline="Black"
        color="#aaffaa00"
        showAnimation="FadeIn"
        hideAnimation="FadeOut">
        <HorizontalLayout>
            <Panel id = "GreenTokens"
                width="70"
                height="280"
                color="#aaffaa00">
                <VerticalLayout>
                    <Button id="FocusBtn" class="TokenButtons" onClick="focusTok">
                        <Image id= "FocusIcon" class="TIcon" image="Focus" preserveAspect="true"></Image>
                    </Button>
                    <Button id="EvadeBtn" class="TokenButtons" onClick="evadeTok">
                        <Image id= "EvadeIcon" class="TIcon" image="Evade" preserveAspect="true"></Image>
                    </Button>
                    <Button id="TargetLockBtn" class="TokenButtons"  onClick="tlTok">
                        <Image id= "TLIcon" class="TIcon" image="TL" preserveAspect="true"></Image>
                    </Button>
                    <Button id="CalculateBtn" class="TokenButtons"  onClick="calcTok">
                        <Image id= "CalculateIcon" class="TIcon" image="Calculate" preserveAspect="true"></Image>
                    </Button>
                    <Button id="ReinforceBtn" class="TokenButtons" onClick="reinforceTok">
                        <Image id= "ReinforceIcon" class="TIcon" image="Reinforce" preserveAspect="true"></Image>
                    </Button>
                </VerticalLayout>
            </Panel>
            <Panel id = "RedTokenShed"
                width="70"
                height="280"
                color="#aaffaa00">
                <VerticalLayout>
                    <Button id="StressBtn" class="TokenButtons" onClick="stressTok" active="true">
                        <image  id= "StressIcon" class="TIcon" image="Stress" preserveAspect="true"></image></Button>
                    <Button id="StrainBtn" class="TokenButtons" onClick="strainTok" active="true">
                        <image  id= "StrainIcon" class="TIcon" image="Strain" preserveAspect="true"></image></Button>
                    <Button id="DepleteBtn" class="TokenButtons" onClick="depleteTok" active="true">
                        <image  id= "DepleteIcon" class="TIcon" image="Deplete" preserveAspect="true"></image></Button>
                    <Button id="IonBtn" class="TokenButtons" onClick="ionTok" active="true">
                        <image  id= "IonIcon" class="TIcon" image="Ion" preserveAspect="true"></image></Button>
                </VerticalLayout>
            </Panel>
            <Panel id = "OrangeTokens"
                width="70"
                height="280"
                rectAlignment="MiddleLeft"
                color="#aaffaa00">
                <VerticalLayout>
                    <Button id="DisarmBtn" class="TokenButtons" onClick="disarmTok" active="true">
                        <image  id= "DisarmIcon" class="TIcon" image="Disarm" preserveAspect="true"></image></Button>
                    <Button id="JamBtn" class="TokenButtons" onClick="jamTok" active="true">
                        <image  id= "JamIcon" class="TIcon" image="Jam" preserveAspect="true"></image></Button>
                    <Button id="TractorBtn" class="TokenButtons" onClick="tractorTok" active="true">
                        <image  id= "TractorIcon" class="TIcon" image="Tractor" preserveAspect="true"></image></Button>
                </VerticalLayout>
            </Panel>
            <Panel id = "BlueTokens"
                width="70"
                height="280"
                rectAlignment="MiddleLeft"
                color="#aaffaa00">
                <VerticalLayout>
                    <Button id="CloakBtn" class="TokenButtons" onClick="cloakTok" active="true">
                        <image  id= "CloakIcon" class="TIcon" image="Cloak" preserveAspect="true"></image></Button>
                </VerticalLayout>
            </Panel>
        </HorizontalLayout>
    </Panel>
</Panel>

<Panel id="proxyPanel"
	active="false"
	width="700"
	height="150"
	position="0 -265 -25"
	color="#aaaaaa00"
	showAnimation="FadeIn"
	hideAnimation="FadeOut"
	raycastTarget="true">
	<VerticalLayout childAlignment="MiddleBottom"
		childForceExpandHeight="false"
		spacing="5">
		<HorizontalLayout childAlignment="MiddleCenter"
			childForceExpandWidth="false"
			spacing="5">
			<Button id="proxyFrontBtn"
				class="RelocButtons"
				active="false">
				<Image id="TalonForIcon"
					class="RIcon"
					image="TalonFront"
					preserveAspect="true"></Image>
			</Button>
			<Button id="proxyCenterBtn"
				class="RelocButtons"
				active="false">
				<Image id="TalonMidIcon"
					class="RIcon"
					image="TalonMid"
					preserveAspect="true"></Image>
			</Button>
			<Button id="proxyBackBtn"
				class="RelocButtons"
				active="false">
				<Image id="TalonReaIcon"
					class="RIcon"
					image="TalonRear"
					preserveAspect="true"></Image>
